    </btn_load>

    <btn_delete x="184" y="427" width="131" height="48">
      <texture>ui_button_main02</texture>
      <text font="graffiti22">ui_mm_delete</text>
      <text_color>
        <e r="227" g="199" b="178"/>
        <t r="180" g="153" b="155"/>
        <d r="106" g="95" b="91"/>
        <h r="0" g="0" b="0"/>
      </text_color>
    </btn_delete>

    <btn_cancel x="314" y="427" width="131" height="48">
      <texture>ui_button_main02</texture>
      <text font="graffiti22">ui_mm_cancel</text>
      <text_color>
        <e r="227" g="199" b="178"/>
        <t r="180" g="153" b="155"/>
        <d r="106" g="95" b="91"/>
        <h r="0" g="0" b="0"/>
      </text_color>
    </btn_cancel>
