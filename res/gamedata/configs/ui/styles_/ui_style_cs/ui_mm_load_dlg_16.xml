    <auto_static x="0" y="0" width="85" height="768" stretch="1">
      <texture>ui_inGame2_back_02w10_l</texture>
    </auto_static>
    <auto_static x="939" y="0" width="85" height="768" stretch="1">
      <texture>ui_inGame2_back_02w10_r</texture>
