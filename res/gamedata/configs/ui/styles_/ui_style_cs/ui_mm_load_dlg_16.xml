<!-- 16:10 ready -->
<w>
  <background x="0" y="0" width="1024" height="768">

    <auto_static x="0" y="0" width="1024" height="430">
      <!-- fullscreen video -->
      <texture x="0" y="0" width="1024" height="430">ui\ui_mm_window_back_crop</texture>
