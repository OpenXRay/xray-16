[smart_terrain]
squad_id = 3
max_population = 7
min_population = 7			;makes smart an absolute priority until min_population met
respawn_params = respawn@bar_visitors
respawn_idle = 86400


[respawn@bar_visitors]
spawn_stalker@novice
spawn_stalker@advanced

[spawn_stalker@novice]
